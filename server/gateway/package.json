{
  "name": "gateway",
  "version": "0.0.1",
  "description": "My Notes App",
  "main": "dist/index.js",
  "scripts": {
  "clean": "rm -rf dist",
  "prebuild": "npm run clean && eslint . --ext .ts --fix",
  "build": "tsc -p tsconfig.build.json && cp -r ../../www dist/www",
  "prestart": "npm run build",
  "start": "node .",
